{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ],
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/.next"
}
